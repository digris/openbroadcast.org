set basepath to "~/code/upgrade.openbroadcast.org/"set envpath to "~/srv/upgrade.openbroadcast.org/"set server_port to 8080tell application "iTerm"	make new terminal	tell the current terminal		activate current session		launch session "django"		tell the last session			write text "cd " & basepath & "website/"			write text "source " & envpath & "bin/activate"			write text "./manage.py runserver 0.0.0.0:" & server_port		end tell	end tell		tell i term application "System Events" to keystroke "d" using command down	tell the current terminal		tell the last session			write text "cd " & basepath & "website/site-static/"			write text "source " & envpath & "bin/activate"			write text "compass watch"					end tell	end tell		tell i term application "System Events" to keystroke "D" using command down	tell the current terminal		tell the last session			write text "cd " & basepath & "website/"			write text "source " & envpath & "bin/activate"			write text "./manage.py celeryd -c 1 -l DEBUG"		end tell	end tell		tell i term application "System Events" to keystroke "D" using command down	tell the current terminal		tell the last session			write text "cd ~/code/openbroadcast/pushy/server/"			write text "node pushy.js"		end tell	end tell		tell i term application "System Events" to keystroke "D" using command down	tell the current terminal		tell the last session			write text "cd ~/srv/echoprint-server/solr"			write text "./run.sh"		end tell	end tell		tell i term application "System Events" to keystroke "D" using command down	tell the current terminal		tell the last session			write text "cd " & basepath & "website/"			write text "source " & envpath & "bin/activate"			write text "clear"		end tell	end tell	end tell